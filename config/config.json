{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "guid-msf-field-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/guidMsf/GuidMsfFieldCustomizer.js",
          "manifest": "./src/extensions/guidMsf/GuidMsfFieldCustomizer.manifest.json"
        }
      ]
    },
    "library-msf-field-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/libraryMsf/LibraryMsfFieldCustomizer.js",
          "manifest": "./src/extensions/libraryMsf/LibraryMsfFieldCustomizer.manifest.json"
        }
      ]
    },
    "progressbar-msf-field-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/progressbarMsf/ProgressbarMsfFieldCustomizer.js",
          "manifest": "./src/extensions/progressbarMsf/ProgressbarMsfFieldCustomizer.manifest.json"
        }
      ]
    },
    "steps-msf-field-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/stepsMsf/StepsMsfFieldCustomizer.js",
          "manifest": "./src/extensions/stepsMsf/StepsMsfFieldCustomizer.manifest.json"
        }
      ]
    },
    "arrowsteps-msf-field-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/arrowstepsMsf/ArrowstepsMsfFieldCustomizer.js",
          "manifest": "./src/extensions/arrowstepsMsf/ArrowstepsMsfFieldCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "GuidMsfFieldCustomizerStrings": "lib/extensions/guidMsf/loc/{locale}.js",
    "LibraryMsfFieldCustomizerStrings": "lib/extensions/libraryMsf/loc/{locale}.js",
    "ProgressbarMsfFieldCustomizerStrings": "lib/extensions/progressbarMsf/loc/{locale}.js",
    "StepsMsfFieldCustomizerStrings": "lib/extensions/stepsMsf/loc/{locale}.js",
    "ArrowstepsMsfFieldCustomizerStrings": "lib/extensions/arrowstepsMsf/loc/{locale}.js"
  }
}
